doctype html
html
	head
		base(href=serviceUrl+'/')
		title #{serviceFullName} (FeatureServer)
		link(href='/css/ESRI.ArcGIS.Rest.css', rel='stylesheet')
	body
		//- Page header
		table#userTable(width='100%')
			tr
				td#titlecell Wrapped Services Directory
		table#navTable(width='100%')
			tr(valign="top")
				td#breadcrumbs breadcrumbs
				td#help(align='right')
					a(href='help.html') Help
					|  | 
					a(href='apiref.html') API Reference

		div.restHeader
			h2 #{serviceFullName} (Feature Server)

		div.restBody
			b View In: 
			a(href='') ArcGIS.com Map
			br
			br
			//- Service Description
			b Service Description: 
			| #{serviceDescription}	
			br
			br
			//- Layers
			b Layers: 
			ul
				li
					a(href='0') xx
					|  (0)
			br
			br
			//- Tables
			b Tables: 
			ul
				li
					a(href='0') xx
					|  (0)
			//- Supported Interfaces
			b Supported Interfaces: 
			a(href='x') REST
			| 		
			a(href='x') SOAP
